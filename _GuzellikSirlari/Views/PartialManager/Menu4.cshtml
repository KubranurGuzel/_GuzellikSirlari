@model List<_GuzellikSirlari.Models.DogalMaskeler> 
@{
    Layout = null;
}
  <!-----4.menü-->
<section id="about" class="tm-content-box">
    <div class="tm-content-box tm-content-box-home">
        <img src="~/Content/img/8000fddf4c4ada52acdf7842c4158b7c_1508411666-b.jpg" alt="Image 1" class="img-fluid tm-welcome-img">
        <div class="tm-welcome-boxes-container">
            <div class="tm-welcome-box">
                <div class="tm-welcome-text">
                    <h2 class="tm-section-title">Doğal Maskeler Nedir?</h2>
                    <p>Makyaj malzemelerine gerek duymadan cilt tonunuzun eşitlenmesidir. </p>
                </div>
                <a class="tm-welcome-link tm-button">Doğal Maskeler </a>
            </div>
            <div class="tm-welcome-box">
                <div class="tm-welcome-text">
                    <h2 class="tm-section-title">Doğal Maskeler Gerekli Mi?</h2>
                    <p>Soğuk havalarda kuru ciltler için gereklidir.</p>
                </div>
                <a class="tm-welcome-link tm-button">Doğal Maskeler </a>
            </div>

        </div>
    </div>
</section>
<div id="gallery1" class="tm-content-box">
    <div class="grid tm-gallery">
        <figure class="effect-bubba">
            <img src="~/Content/img/dogal_maskeler1.jpg" alt="Image 8" class="img-fluid">
            <figcaption>
                <h2>Doğal <br /> <span>Maskeler</span></h2>
    
                <a href="~/Content/img/dogal_maskeler1.jpg">View more</a>
            </figcaption>
        </figure>

        <figure class="effect-bubba">
            <img src="~/Content/img/dogal_maskeler2.png" alt="Image 7" class="img-fluid">
            <figcaption>
                <h2>Doğal <br /> <span>Maskeler</span></h2>
         
                <a href="~/Content/img/dogal_maskeler2.png">View more</a>
            </figcaption>
        </figure>

        <figure class="effect-bubba">
            <img src="~/Content/img/dogal_maskeler3.jpg" alt="Image 5" class="img-fluid">
            <figcaption>
                <h2>Doğal <br /> <span>Maskeler</span></h2>
         
                <a href="~/Content/img/dogal_maskeler3.jpg">View more</a>
            </figcaption>
        </figure>

  
    </div>
</div>

<div id="services2" class="tm-content-box tm-gray-bg tm-services">

    <div class="tm-services-img-container">
        <img src="~/Content/img/dogal_maskelerdik.jpg" alt="Green Field" class="img-fluid tm-services-img">
    </div>

    <div class="tm-box-pad tm-services-description-container">
        <h2 class="tm-section-title"><span class="tm-blue-text">Doğal Maskeler</span> </h2>
        @foreach (var item in Model)
        {
            <p class="tm-section-description">@Html.ActionLink(@item.DogalMaskelerAdi, "DetaySayfasiMenu4", new { DogalMaskelerId = item.DogalMaskelerId })</p>
        }
    </div> 

</div>



