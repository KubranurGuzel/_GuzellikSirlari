}
@using Microsoft.AspNet.Identity
@if (Session["KullaniciId"] != null)
{
    <div id="about1" class="tm-content-box">
        <div class="tm-box-pad tm-bordered-box">

            <h4>Doğal Maskeler</h4>

            <div>

                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        Doğal Maskeler Adı
                    </dt>

                    <dd>
