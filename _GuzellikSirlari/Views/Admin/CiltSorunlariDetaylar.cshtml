@model _GuzellikSirlari.Models.CiltSorunlari
@{
    ViewBag.Title = "CiltSorunlariDetaylar";
    Layout = "~/Views/Shared/_Layout3.cshtml";
}

@using Microsoft.AspNet.Identity
@if (Session["KullaniciId"] != null)
{
    <div id="about1" class="tm-content-box">
        <div class="tm-box-pad tm-bordered-box">

            <h4>Cilt Sorunları</h4>

            <div>

                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        Cilt Sorunları Adı
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.CiltSorunlariAdi)
                    </dd>

                    <dt>
                        Cilt Sorunları Açıklama
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.CiltSorunlariAciklama)
                    </dd>

                </dl>
            </div>
        </div>
    </div>
}

