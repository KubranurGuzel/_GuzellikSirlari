@model _GuzellikSirlari.Models.Iletisim
@{
    ViewBag.Title = "IletisimDetaylar";
    Layout = "~/Views/Shared/_Layout3.cshtml";
}
@using Microsoft.AspNet.Identity
@if (Session["KullaniciId"] != null)
{
    <div id="about1" class="tm-content-box">
        <div class="tm-box-pad tm-bordered-box">

            <h4>İletişim</h4>

            <div>

                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        Adı
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Adi)
                    </dd>

                    <dt>
                        Mail Adresi
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Email)
                    </dd>
                    <dt>
                        Açıklama
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Aciklama)
                    </dd>

                </dl>
            </div>
        </div>
    </div>
}

